{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,js,png,jpg,jpeg,gif,svg,webp,PNG,JPG}", "/api/*"]
  },
  "globalHeaders": {
    "cache-control": "must-revalidate, max-age=3600"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
